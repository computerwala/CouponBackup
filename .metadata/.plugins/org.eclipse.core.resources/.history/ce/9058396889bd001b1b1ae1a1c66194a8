package com.beta.usermicroservice.model;

import java.sql.Date;
import java.util.Set;


public class UserOrders {

	private Date orderDate;
	private Double total;
	private Double discount;
	private Set<UserOrderDetail> userOrderDetails;
	public UserOrders(Date orderDate, Double total, Double discount, Set<UserOrderDetail> userOrderDetails) {
		super();
		this.orderDate = orderDate;
		this.total = total;
		this.discount = discount;
		this.userOrderDetails = userOrderDetails;
	}
	public Date getOrderDate() {
		return orderDate;
	}
	public void setOrderDate(Date orderDate) {
		this.orderDate = orderDate;
	}
	public Double getTotal() {
		return total;
	}
	public void setTotal(Double total) {
		this.total = total;
	}
	public Double getDiscount() {
		return discount;
	}
	public void setDiscount(Double discount) {
		this.discount = discount;
	}
	public Set<UserOrderDetail> getUserOrderDetails() {
		return userOrderDetails;
	}
	public void setUserOrderDetails(Set<UserOrderDetail> userOrderDetails) {
		this.userOrderDetails = userOrderDetails;
	}
	
	
	
}
